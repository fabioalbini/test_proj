{
  "family": "capistrano-deploy-staging",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "cap-deploy",
      "essential": true,
      "memory": 0.5,
      "entryPoint": [
        "bundle exec cap staging deploy"
      ]
    }
  ]
}
